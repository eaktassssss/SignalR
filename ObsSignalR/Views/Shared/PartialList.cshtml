@model IEnumerable<ObsSignalR.Models.Context.Students>
<table class="table">
    <tr>
        <th>
            FirstName
        </th>
        <th>
           LastName
        </th>
        <th>
            IdentityNumber
        </th>
        <th>
          PhoneNumber
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
               @item.FirstName
            </td>
            <td>
                @item.LastName 
            </td>
            <td>
                @item.IdentityNumber
            </td>
            <td>
                @item.PhoneNumber
            </td>
            @*<td>
                <a href="@Url.Action("Edit", "Obs", new {id = item.Id})">Düzenle</a>
                <a href="@Url.Action("Delete", "Obs", new {id = item.Id})">Kaldır</a>
            </td>*@
        </tr>
    }

</table>
